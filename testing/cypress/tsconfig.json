{
  "compilerOptions": {
    "allowJs": true,
    "module": "es6",
    "target": "es5",
    "jsx": "preserve",
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "types": [
      "cypress",
      "node"
    ]
  },
}
